   "source": [
    "from pykrx import stock\n",
    "start_date = '2019-01-01'\n",
    "end_date   = '2019-12-31'\n",
    "\n",
    "# k10_historical_price = stock.get_market_ohlcv(start_date, end_date, '005930')\n",
    "# k10_historical_price\n",
    "\n",
    "ohlcv_dict = dict()\n",
    "for stock_cd in k10_component:\n",
    "    ohlcv_s = stock.get_market_ohlcv(start_date, end_date, stock_cd)['종가']\n",
    "    ohlcv_dict[stock_cd] = ohlcv_s\n",
    "k10_ohlcv = pd.DataFrame(ohlcv_dict)\n",
    "k10_ohlcv.sort_index(inplace=True)  \n",
    "k10_ohlcv.head(3)"
   ]
