  {
   "cell_type": "code",
   "execution_count": 24,
   "metadata": {},
   "outputs": [],
   "source": [
    "CU = 50000                           # 설정/환매 단위\n",
    "base_date = dt.date(2019, 1, 2)      # 설정 기준일\n",
    "volume = 1000000                     # 최초 설정수량\n",
    "interest_rate = 0.02                 # 이자율"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 25,
   "metadata": {},
   "outputs": [],
   "source": [
    "def creation_redemption(v):\n",
    "    creation = np.random.randint(0, 5) * CU\n",
    "    if v>500000:\n",
    "        redemption = np.random.randint(0, 5) * CU\n",
    "    else:\n",
    "        redemption = 0\n",
    "    volume = v + creation - redemption  #이전총좌수 + 신규좌수 - 환매좌수 == 오늘 총좌수\n",
    "    return(creation, redemption, volume)"
   ]
  },
